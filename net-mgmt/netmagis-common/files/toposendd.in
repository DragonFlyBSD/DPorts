#! /bin/sh
#
# $FreeBSD: head/net-mgmt/netmagis-common/files/toposendd.in 306707 2012-10-30 20:27:10Z crees $
#

# PROVIDE: toposendd
# REQUIRE: LOGIN
# KEYWORD: shutdown

#
# Add the following lines to /etc/rc.conf to enable toposendd:
#
# toposendd_enable="YES"
#
# # Optionnal variables:
# toposendd_flags=""
# toposendd_user="rancid"
#

. /etc/rc.subr

name=toposendd
rcvar=toposendd_enable

load_rc_config $name

: ${toposendd_enable:=NO}
: ${toposendd_user:=rancid}

command_interpreter=%%TCLSH%%
command=%%PREFIX%%/sbin/toposendd

run_rc_command $1
